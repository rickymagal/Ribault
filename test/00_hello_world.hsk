main u =
  let
    src u1 =
      super single input (u1) output (h)
        #BEGINSUPER
        h = "Hello, World!"
        #ENDSUPER
    ;
    par x =
      super parallel input (x) output (y)
        #BEGINSUPER
        y = x
        #ENDSUPER
    ;
    sink s =
      super single input (s) output (status)
        #BEGINSUPER
        puts(s);
        status = 0
        #ENDSUPER
    ;
  in sink (par (src u));
