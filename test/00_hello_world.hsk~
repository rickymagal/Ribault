src u =
  super single input (u) output (h)
    #BEGINSUPER
    h = "Hello, World!"
    #ENDSUPER
;

par x =
  super parallel input (x) output (y)
    #BEGINSUPER
    y = x
    #ENDSUPER
;

sink s =
  super single input (s) output (status)
    #BEGINSUPER
    puts(s);
    status = 0
    #ENDSUPER
;

main = sink (par (src 0));
